<section class="container">
  <div class="row">
    <div class="col-md-12">
      <h3>
        <%= fa_icon 'table' %> <%= @import.name %> logs
        <%= link_to 'Back', imported_events_path, class: 'btn btn-default pull-right' %>
      </h3>
      
      <table class="table table-striped table-condensed">
        <thead>
        <tr>
          <th>Row</th>
          <th>Status</th>
          <th>Message</th>
        </tr>
        </thead>

        <tbody>
        <% @import_logs.each do |log| %>
            <tr>
              <td><span class="badge"><%= log.row %></span></td>
              <td>
	              <% if log.status == 200 %>
	              	<span class="label label-success">Success</span>
	              <% else %>
	              	<span class="label label-danger">Error</span>
	              <% end %>
              </td>
              <td><%= log.message %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</section>