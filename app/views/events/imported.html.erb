<section class="container">
  <div class="row">
    <div class="col-md-12">
      <h3>
        <%= fa_icon 'file-excel-o' %> Imported files
      </h3>
      
      <table class="table table-striped table-condensed">
        <thead>
        <tr>
          <th></th>
          <th>Name</th>
          <th>Type</th>
        </tr>
        </thead>

        <tbody>
        <% @imported.each do |file| %>
            <tr>
              <td><%= file.id %></td>
              <td><%= link_to file.name, imported_log_events_path(file) %></td>
              <td><%= file.import_type %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</section>